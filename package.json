{
  "name": "git-add-cli",
  "description": "Interactive `git add` command",
  "version": "0.2.1",
  "main": "lib/index",
  "bin": {
    "git-add": "bin/git-add"
  },
  "files": [
    "*.md",
    "bin",
    "lib",
  ],
  "scripts": {
    "start": "npm test -- -w --watch-extensions js",
    "test": "mocha -t 10000 --compilers js:babel-core/register test",
    "prebuild": "rimraf lib",
    "build": "babel src -d lib",
    "prepublish": "npm run build"
  },
  "repository": "git@github.com:Cap32/git-add-cli.git",
  "author": "Cap32 <webb.png@gmail.com>",
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "git",
    "cli"
  ],
  "licenses": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^2.0.0",
    "mz": "^2.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
